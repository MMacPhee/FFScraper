qb_list.get_list("qb")
rb_list.get_list("rb")
wr_list.get_list("wr")

print(len(qb_list.content))
print(len(rb_list.content))
print(len(wr_list.content))
print(qb_list.name)
print(rb_list.name)
print(wr_list.name)
